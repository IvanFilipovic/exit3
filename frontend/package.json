{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "lintfix": "eslint . --fix && prettier --write --list-different .",
    "typecheck": "nuxt typecheck",
    "ci": "npm ci",
    "verify": "npm run lint && npm run typecheck",
    "check-updates": "npx npm-check-updates",
    "update:safe": "npx npm-check-updates -u --target patch",
    "update:minor": "npx npm-check-updates -u --target minor",
    "audit": "npm audit --production"
  },
  "dependencies": {
    "@dargmuesli/nuxt-cookie-control": "^8.5.4",
    "@gtm-support/vue-gtm": "^3.1.0",
    "@nuxt/icon": "^1.10.3",
    "@nuxt/image": "^1.9.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^9.5.6",
    "@nuxtjs/robots": "^5.5.1",
    "@nuxtjs/seo": "^2.2.0",
    "@nuxtjs/sitemap": "^7.4.3",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@sentry/vue": "^10.32.1",
    "@unhead/schema": "^1.11.20",
    "@unhead/vue": "^1.11.20",
    "@vueuse/motion": "^3.0.3",
    "ipx": "^3.0.1",
    "js-cookie": "^3.0.5",
    "nuxt-aos": "^1.2.5",
    "nuxt-csurf": "^1.6.5",
    "nuxt-delay-hydration": "^1.3.8",
    "nuxt-lazy-hydrate": "^1.0.0",
    "nuxt-mdi": "^2.1.1",
    "nuxt-rate-limit": "^1.2.0",
    "nuxt-simple-robots": "^4.0.0",
    "nuxt-vitalizer": "^0.10.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animated": "^2.0.0",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@iconify-json/devicon": "^1.2.17",
    "@iconify-json/entypo": "^1.2.1",
    "@nuxt/eslint": "^1.0.0",
    "@zadigetvoltaire/nuxt-gtm": "^0.0.13",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "nuxt": "^3.17.0",
    "nuxt-anchorscroll": "^1.0.5",
    "nuxt-headlessui": "^1.2.0",
    "nuxt-particles": "^0.3.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  }
